{
  "name": "recipe-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/recipe-page/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/apps/recipe-page",
        "compiler": "swc",
        "main": "apps/recipe-page/src/main.ts",
        "tsConfig": "apps/recipe-page/tsconfig.app.json",
        "webpackConfig": "apps/recipe-page/webpack.config.js",
        "assets": [
          "apps/recipe-page/src/favicon.png",
          "apps/recipe-page/src/assets"
        ],
        "index": "apps/recipe-page/src/index.html",
        "baseHref": "/",
        "styles": ["apps/recipe-page/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "apps/recipe-page/src/environments/environment.ts",
              "with": "apps/recipe-page/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "recipe-page:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "recipe-page:build:development"
        },
        "production": {
          "buildTarget": "recipe-page:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/recipe-page/jest.config.ts"
      }
    }
  }
}
